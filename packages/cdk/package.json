{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk --profile=knot",
    "cdk:deploy": "npx cdk deploy --profile=knot",
    "cdk:deploy:prod": "DEPLOY_ENV=prod npx cdk deploy --profile=knot",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.1",
    "@graphql-codegen/typescript": "2.4.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "aws-cdk": "2.3.0",
    "eslint": "^8.6.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "ethereum-waffle": "^3.4.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "@graphql-codegen/typescript-resolvers": "2.4.3"
  },
  "dependencies": {
    "@aws-cdk/aws-appsync-alpha": "^2.3.0-alpha.0",
    "aws-cdk-lib": "2.3.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.16"
  }
}
